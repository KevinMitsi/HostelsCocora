<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="408.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hostelscocora.Controllers.AgregarProductoController">
   <children>
      <Label layoutX="218.0" layoutY="51.0" text="REGISTRO DE PRODUCTOS">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label layoutX="273.0" layoutY="97.0" text="Categoría:" />
      <ComboBox fx:id="cbxCategoriaProducto" layoutX="225.0" layoutY="128.0" prefWidth="150.0" />
      <Label layoutX="253.0" layoutY="172.0" text="Tipo de Producto:" />
      <ComboBox fx:id="cbxTipoProducto" layoutX="225.0" layoutY="207.0" prefWidth="150.0" />
      <Label layoutX="227.0" layoutY="261.0" text="Identificación del producto:" />
      <TextField fx:id="tfIdentificacion" layoutX="225.0" layoutY="297.0" promptText="Identificación" />
      <Button fx:id="btnEnviar" layoutX="274.0" layoutY="357.0" mnemonicParsing="false" onAction="#onEnviarButtonClick" text="Enviar" />
      <Button fx:id="btnRegresar" layoutX="30.0" layoutY="359.0" mnemonicParsing="false" onAction="#onRegresarButtonClick" text="Regresar">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
